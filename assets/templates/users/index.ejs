<div class="default-page">
    <div class="header">
        <h1 id="main-title" class="container"><%= __('A brand new app.') %></h1>
        <h3 class="container">You're looking at: <code><%= view.pathFromApp + '.' +view.ext %></code></h3>
    </div>
    <div class="main container clearfix">
        <table class="table" style="width: 100%">
        <colgroup span=3>
            <col style="width: 33%" />
            <col style="width: 33%" />
            <col style="width: 33%" />
        </colgroup>
        <tr>
            <th>
                Name
            </th>
            <th>
                Username
            </th>
            <th>
                Email
            </th>
            
        </tr>
        <% _.each(model, function(user) { %>
        <tr>
            <td>
                <%= user.name %>
            </td>
            <td>
                <%= user.username %>
            </td>
            <td>
                <%= user.email %>
            </td>
        </tr>
        <% }) %>
    </table>
    </div>
    <div class="text-right">
        <button type="button" role="button" class="btn btn-primary">
            Add New User
        </button>
    </div>
</div>